local timeZoneOffset = 25200 -- 7 hours ahead of UTC

local currentUnixTime = os.time()
local currentDateTime = os.date("*t", currentUnixTime + timeZoneOffset)

local formattedTime = string.format("%02d:%02d:%02d", currentDateTime.hour, currentDateTime.min, currentDateTime.sec)
local formattedDate = string.format("%d-%02d-%02d", currentDateTime.year, currentDateTime.month, currentDateTime.day)

-- Get the number of players currently in the game
local playerCount = #game:GetService("Players"):GetPlayers()

local bossIsnah = "https://discord.com/api/webhooks/1179059848571273297/dVdJuEcXfKJiyMbXeKDGiS2RC-LLdKlCq7YXdKwwM83CFkrhyqupVmNiwdtMSt1d0dgt"

local bossSonic = "https://discord.com/api/webhooks/1139481458185089094/30YbfrzONnbhpGgbzstQioEKt9Rlf9UHmKqlepH95DYSFo1hV3bVnmP6rdIlb78N8j22"

local bossSkys = "https://discord.com/api/webhooks/1175376916690243684/j6Ik9uPg61j2gy5UfxlrFtcXimklZJTRf799sbV8unZxADgy5B-BDRiyoyZkNBrSKDXx"

local bossKevin = "https://discord.com/api/webhooks/1145977032992366602/HCSlHVUb-mBVtfDgZZPVLugPejt7QjQCVQDcaBBP18gzYrmrlSuQGhwqogIxXPZuqhjk"

local bossNamesGroups = {
    ["2753915549"] = {"The Saw", "Greybeard"},      -- First Sea
    ["4442272183"] = {"Darkbeard", "Order", "Cursed Captain"},  -- Second Sea
    ["7449423635"] = {"rip_indra True Form", "Soul Reaper", "Cake Prince", "Dough King", "Terror Shark", "Leviathan"}  -- Third Sea
}

local worldBossNames = bossNamesGroups[tostring(game.PlaceId)]

local locatedSea
if game.PlaceId == 2753915549 then
    locatedSea = "``` First Sea ```"
elseif game.PlaceId == 4442272183 then
    locatedSea = "``` Second Sea ```"
elseif game.PlaceId == 7449423635 then
    locatedSea = "``` Third Sea ```"
else
    locatedSea = "Unknown Sea"
end

for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
    if table.find(worldBossNames, v.Name) then
        local embed = {
            ["title"] = "Boss Hunter ( RAID BOSS )",
            ["description"] = "Hello, I'm Bot For Boss Finder",
            ["color"] = 15844367, -- 10181046
            ["fields"] = {
                {
                    ["name"] = "**[👹] Boss Name:**",
                    ["value"] = v.Name 
                },
                {
                    ["name"] = "**[📌] Located:**",
                    ["value"] = locatedSea
                },
                {
                    ["name"] = "**[👥] Players In Server:**",
                    ["value"] = "```".. playerCount .."/12 ```"
                },
                {
                    ["name"] = "**[🔗] Server Job Id:**",
                    ["value"] = "```game.ReplicatedStorage['__ServerBrowser']:InvokeServer('teleport','"..game.JobId.."')```"
                }
            },
            ["footer"] = {
                ["text"] = " | "..formattedDate.." | Note: Execute Script (Server Job Id) To Join Server"
            }
        }
        SendMessageEMBED(bossIsnah, embed)
        local embed = {
            ["title"] = "Boss Hunter ( RAID BOSS )",
            ["description"] = "Hello, I'm Bot For Boss Finder",
            ["color"] = 15844367, -- 10181046
            ["fields"] = {
                {
                    ["name"] = "**[👹] Boss Name:**",
                    ["value"] = v.Name 
                },
