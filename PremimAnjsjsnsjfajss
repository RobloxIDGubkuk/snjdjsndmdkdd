    for i,v in pairs(game.Workspace:GetDescendants()) do -- grabs everything from workspace
    if v.ClassName == 'TouchTransmitter' and v.Parent.Name == 'Handle' then -- checks if it has a handle and if its a touchtransmitter
        local BillboardGui = Instance.new('BillboardGui') -- Makes Billboardgui
        local TextLabel = Instance.new('TextLabel') -- makes text label
        
        BillboardGui.Parent = v.Parent -- what the billboardgui goes into
        BillboardGui.AlwaysOnTop = true -- if its on top or not
        BillboardGui.Size = UDim2.new(0, 60, 0, 60) -- size of it
        BillboardGui.StudsOffset = Vector3.new(0,2,0)
        
        TextLabel.Parent = BillboardGui -- putting textlabel into billboardgui
        TextLabel.BackgroundColor3 = Color3.new(1,1,1) -- color
        TextLabel.BackgroundTransparency = 1 -- transparency
        TextLabel.Size = UDim2.new(1, 0, 1, 0) -- size
        TextLabel.Text = v.Parent.Parent.Name -- what the label says
        TextLabel.TextColor3 = Color3.new(1, 0, 0) -- color
        TextLabel.TextScaled = true -- if the text is scaled or not
    end
    end
    wait(0.5)
    Notification.new("<Color=Yellow>!! FRUIT FINDER V3 ( PREMIUM ) !!<Color=/>"):Display()
    wait(0.5)
    Notification.new("<Color=Green>!! Collect Only Fruit Spawn : ✓ !!<Color=/>"):Display()
    wait(0.5)
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Mengatur properti GUI
ScreenGui.IgnoreGuiInset = true
ScreenGui.Enabled = true
ScreenGui.ResetOnSpawn = false

-- Variabel untuk menyimpan informasi tentang penggeseran GUI
local isDragging = true
local offset = Vector2.new(0, 0)

-- Membuat latar belakang hitam
local Background = Instance.new("Frame")
Background.Name = "Background"
Background.Size = UDim2.new(0, 250, 0, 200)
Background.Position = UDim2.new(0.5, -200, 0.5, -150)
Background.AnchorPoint = Vector2.new(0.5, 0.5)
Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Background.BorderSizePixel = 0
Background.Parent = ScreenGui

-- Membuat area biru di sekeliling latar belakang
local BlueBorder = Instance.new("Frame")
BlueBorder.Name = "BlueBorder"
BlueBorder.Size = UDim2.new(1, 6, 1, 6)
BlueBorder.Position = UDim2.new(0, -3, 0, -3)
BlueBorder.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
BlueBorder.BorderSizePixel = 0
BlueBorder.Parent = Background

-- Membuat area putih di dalam latar belakang
local WhiteArea = Instance.new("Frame")
WhiteArea.Name = "WhiteArea"
WhiteArea.Size = UDim2.new(1, -12, 1, -12)
WhiteArea.Position = UDim2.new(0, 6, 0, 6)
WhiteArea.BackgroundColor3 = Color3.fromRGB(1, 1, 1)
WhiteArea.BorderSizePixel = 0
WhiteArea.Parent = BlueBorder

-- Menambahkan fungsi untuk menggeser GUI
Background.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = true
        offset = input.Position - Background.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                isDragging = false
            end
        end)
    end
end)

Background.InputChanged:Connect(function(input)
    if isDragging then
        Background.Position = UDim2.new(0, input.Position.X - offset.X, 0, input.Position.Y - offset.Y)
    end
end)

-- Menambahkan baris teks pertama
local TextLabel1 = Instance.new("TextLabel")
TextLabel1.Name = "Text1"
TextLabel1.Size = UDim2.new(1, -10, 0, 20)
TextLabel1.Position = UDim2.new(0, 5, 0, 11)
TextLabel1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel1.BorderSizePixel = 0
TextLabel1.Text = "Welcome "..game.Players.LocalPlayer.DisplayName..""
TextLabel1.Parent = WhiteArea

