                 end
             end
             childinstance:GetPropertyChangedSignal("Visible"):Connect(function()
                 if childinstance.Visible then
                     if childinstance.TitleFrame.ErrorTitle.Text == "Teleport Failed" then
                         HopServer()
                         wait(0.1)
