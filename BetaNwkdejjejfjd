                ["description"] = embed.description,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = embed.footer.text
                }
            }
        }
    }
    local body = http:JSONEncode(data)
    local response = request({
        Url = bossIsnah,
        Method = "POST",
        Headers = headers,
        Body = body
    })
    print("Sent -- Raid Boss")
end

